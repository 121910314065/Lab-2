x=int(input("enter first number"))
y=int(input("enter second number"))
def lcm(x, y):
   if x > y:
       z = x
   else:
       z = y

   while(True):
       if((z % x == 0) and (z % y == 0)):
           lcm = z
           break
       z += 1

   return lcm
print(lcm(x,y))

def gcd(x,y):
    i = 1
    while (i <= x and i <= y):
        if (x % i == 0 and y % i == 0):
            gcd = i
        i = i + 1
    return gcd
print(gcd(x,y))
